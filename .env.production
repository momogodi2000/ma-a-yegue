# Production Environment Variables
FIREBASE_PROJECT_ID=Ma’a yegue-production
FIREBASE_API_KEY=your_production_firebase_api_key
FIREBASE_MESSAGING_SENDER_ID=your_production_sender_id
FIREBASE_STORAGE_BUCKET=Ma’a yegue-production.firebasestorage.app
FIREBASE_AUTH_DOMAIN=Ma’a yegue-production.firebaseapp.com

# AI Services (Production)
GEMINI_API_KEY=your_production_gemini_api_key
OPENAI_API_KEY=your_production_openai_api_key

# Payment Services (Production)
CAMPAY_API_KEY=your_production_campay_api_key
CAMPAY_SECRET_KEY=your_production_campay_secret_key
NOUPAI_API_KEY=your_production_noupai_api_key
NOUPAI_SECRET_KEY=your_production_noupai_secret_key

# App Configuration
APP_VERSION=1.0.0
ENVIRONMENT=production
DEBUG_MODE=false
ANALYTICS_ENABLED=true
CRASHLYTICS_ENABLED=true

# Feature Flags (Production)
ENABLE_AI_FEATURES=true
ENABLE_PREMIUM_FEATURES=true
ENABLE_OFFLINE_MODE=true
ENABLE_SOCIAL_FEATURES=true
ENABLE_NOTIFICATIONS=true
ENABLE_BETA_FEATURES=false

# Database Configuration
CACHE_SIZE_MB=100
OFFLINE_SYNC_ENABLED=true
AUTO_BACKUP_ENABLED=true
SYNC_INTERVAL_MINUTES=15

# Logging (Production)
LOG_LEVEL=error
REMOTE_LOGGING_ENABLED=true
LOCAL_LOGGING_ENABLED=false
LOG_RETENTION_DAYS=30

# Performance
MAX_CONCURRENT_SYNCS=3
API_TIMEOUT_SECONDS=30
MAX_RETRY_ATTEMPTS=3
BACKGROUND_SYNC_ENABLED=true

# Security
ENABLE_SSL_PINNING=true
ENABLE_JAILBREAK_DETECTION=true
ENABLE_APP_INTEGRITY_CHECK=true
SESSION_TIMEOUT_MINUTES=30

# Content Delivery
CDN_BASE_URL=https://cdn.Ma’a yegue.com
ASSETS_VERSION=1.0.0
IMAGE_COMPRESSION_QUALITY=80
AUDIO_COMPRESSION_BITRATE=128

# Analytics & Monitoring
SENTRY_DSN=your_production_sentry_dsn
MIXPANEL_TOKEN=your_production_mixpanel_token
AMPLITUDE_API_KEY=your_production_amplitude_key

# Subscription & Billing
STRIPE_PUBLISHABLE_KEY=your_production_stripe_key
REVENUE_CAT_API_KEY=your_production_revenue_cat_key

# Social & Sharing
FACEBOOK_APP_ID=your_production_facebook_app_id
TWITTER_API_KEY=your_production_twitter_key
INSTAGRAM_CLIENT_ID=your_production_instagram_id

# Support & Feedback
INTERCOM_APP_ID=your_production_intercom_id
ZENDESK_SUBDOMAIN=Ma’a yegue
FEEDBACK_EMAIL=support@Ma’a yegue.com