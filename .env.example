# Environment Configuration Template
# Copy this file to .env and fill in your actual values

# App Configuration
APP_ENV=development
APP_NAME=Ma’a yegue
APP_VERSION=1.0.0
BASE_URL=https://Ma’a yegue.app

# Firebase Configuration
FIREBASE_API_KEY=your_firebase_api_key_here
FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_STORAGE_BUCKET=your_project.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your_sender_id
FIREBASE_APP_ID=your_app_id

# Gemini AI Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# CamPay Payment Gateway
CAMPAY_BASE_URL=https://api.campay.net
CAMPAY_API_KEY=your_campay_api_key
CAMPAY_SECRET=your_campay_secret
CAMPAY_WEBHOOK_SECRET=your_campay_webhook_secret

# NouPai Payment Gateway
NOUPAI_BASE_URL=https://api.noupai.com
NOUPAI_API_KEY=your_noupai_api_key
NOUPAI_WEBHOOK_SECRET=your_noupai_webhook_secret
